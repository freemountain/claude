  },
  "devDependencies": {
    "@types/fs-extra": "^3.0.3",
    "@types/ramda": "^0.24.0",
    "@types/source-map-support": "^0.4.0",
    "@types/uuid": "^3.0.0",
    "@types/dockerode": "^2.4.1",
    "@types/lodash": "^4.14.66",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "docker-events": "^0.0.2",
    "dockerode": "^2.5.0",
    "fs-extra": "^3.0.1",
    "lodash": "^4.17.4",
    "ramda": "^0.24.1",
    "source-map-support": "^0.4.15",
    "uuid": "^3.1.0"
  }
}